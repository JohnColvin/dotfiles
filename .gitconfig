[hub]
  protocol = https

[credential]
  helper = /opt/boxen/bin/boxen-git-credential

[core]
  excludesfile = /opt/boxen/config/git/gitignore
  editor = subl3 -w

[user]
  name = John Colvin
  email = colvin.john@gmail.com

[push]
  default = simple

[alias]
  st = status
  co = checkout
  put = push origin HEAD
  jedi = push --force origin
